#/bin/sh

# CREATE SYMLINKS
do ls -s .babunrc ../.babunrc;
do ls -s .bash_profile ../.bash_profile;
do ls -s .bashrc ../.bashrc;
do ls -s .gitconfig ../.gitconfig;
do ls -s .inputrc ../.inputrc;
do ls -s .minttyrc ../.minttyrc;
do ls -s .oh-my-zsh ../.oh-my-zsh;
do ls -s .profile ../.profile;
do ls -s .tmux.conf ../.tmux.conf;
do ls -s .vim ../.vim;
do ls -s .viminfo ../.viminfo;
do ls -s .vimrc ../.vimrc;
do ls -s .zshrc ../.zshrc;
